# Git related
alias g='git'
alias gp='git up && git push origin'
alias gc='git c'
alias gst='git st'
alias gaa='git add --all'
alias ga='git add'
alias gph='git push heroku'
alias gup='git up'
alias glg='git lg'
alias gdfc='git diff --cached --color-words'
alias gdf='git diff --color-words'


alias tmux="tmux -2"
alias zr='source ~/.zshrc'
alias ze='vim ~/.zshrc'
alias clar='clear'
alias rake='noglob rake'
alias http='noglob http'
alias zexports='vim ~/.zsh/zsh-exports'
alias zaliases='vim ~/.zsh/zsh-aliases'
alias rapps='cd ~/Documents/rails-apps'
alias devops='cd ~/Documents/devops'
alias vim='vim -w ~/.vimlog "$@"'
alias dotfiles='vim ~/dotfiles'
